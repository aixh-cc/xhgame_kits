{
    "name": "xhgame_plugin",
    "version": "0.0.1",
    "package_version": 2,
    "type": "module",
    "main": "./dist/browser.cjs",
    "scripts": {
        "nodedev": "tsx src/nodejs/main.ts",
        "dev": "vite serve --mode development",
        "dev:build": "npm run build -- --mode development",
        "build": "vue-tsc -b && vite build",
        "preview": "vite preview"
    },
    "dependencies": {
        "adm-zip": "^0.5.16",
        "archiver": "^5.3.1",
        "cors": "^2.8.5",
        "element-plus": "^2.8.6",
        "express": "^4.18.2",
        "multer": "^1.4.5-lts.1",
        "tsrpc-browser": "^3.4.1",
        "tsrpc-miniapp": "^3.4.1",
        "vue": "^3.4.37",
        "yauzl": "^2.10.0"
    },
    "devDependencies": {
        "@cocos-fe/vite-plugin-cocos-panel": "^1.0.3",
        "@cocos/creator-types": "^3.8.6",
        "@types/adm-zip": "^0.5.7",
        "@types/archiver": "^5.3.2",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/multer": "^1.4.7",
        "@types/node": "^22.15.21",
        "@types/yauzl": "^2.10.0",
        "@vitejs/plugin-vue": "^5.2.2",
        "@vue/tsconfig": "^0.7.0",
        "rollup-plugin-node-externals": "^7.1.3",
        "ts-node": "^10.9.1",
        "tsx": "^4.20.6",
        "typescript": "^5.8.2",
        "unplugin-auto-import": "^0.18.6",
        "unplugin-vue-components": "^0.27.5",
        "vite": "^5.4.1",
        "vue-tsc": "^2.1.10"
    },
    "contributions": {
        "asset-db": {
            "mount": {
                "path": "./assets",
                "readonly": false
            }
        },
        "menu": [
            {
                "path": "i18n:menu.panel/xhgame_plugin",
                "label": "i18n:xhgame_plugin.title",
                "message": "open"
            },
            {
                "path": "i18n:menu.panel/xhgame_plugin",
                "label": "send to panel",
                "message": "increase"
            }
        ],
        "messages": {
            "open": {
                "methods": [
                    "open"
                ]
            },
            "get-version": {
                "methods": [
                    "getVersion"
                ]
            },
            "increase": {
                "methods": [
                    "default.increase"
                ]
            },
            "install-from-assets": {
                "methods": [
                    "installFromAssets"
                ]
            },
            "get-installed-components": {
                "methods": [
                    "getInstalledComponents"
                ]
            },
            "remove-installed-component": {
                "methods": [
                    "removeInstalledComponent"
                ]
            },
            "uninstall-component": {
                "methods": [
                    "uninstallComponent"
                ]
            },
            "get-local-components": {
                "methods": [
                    "getLocalComponents"
                ]
            },
            "install-local-component": {
                "methods": [
                    "installLocalComponent"
                ]
            },
            "checkBackupExists": {
                "methods": [
                    "checkBackupExists"
                ]
            },
            "restoreFromBackup": {
                "methods": [
                    "restoreFromBackup"
                ]
            }
        }
    },
    "panels": {
        "default": {
            "main": "./dist/panel.cjs",
            "size": {
                "min-width": 400,
                "min-height": 300,
                "width": 800,
                "height": 700
            }
        }
    },
    "author": "Cocos Creator"
}